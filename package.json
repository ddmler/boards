    "cross-env": "^5.2.0",
    "eslint": "^4.19.1",
    "eslint-plugin-vue": "^4.5.0",
    "laravel-mix": "^2.1.11",
    "lodash": "^4.17.10",
    "popper.js": "^1.14.3",
    "stylelint": "^9.3.0",
    "stylelint-config-sass-guidelines": "^5.0.0",
    "vue": "^2.5.16",
    "vue-axios": "^2.1.1",
